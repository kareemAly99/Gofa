  <material name="abb_dark_gray"><color rgba="0.3 0.3 0.3 1.0"/></material>
  <material name="gripper_and_cathode"><color rgba="0.5 0.5 0.5 1.0"/></material>

  <!-- Base link -->
  <link name="base_link">
